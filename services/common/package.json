{
  "name": "@bam/common",
  "version": "1.0.0",
  "author": "Eddy",
  "license": "MIT",
  "scripts": {
    "build": "tsc --noEmit false",
    "test": "jest",
    "watch": "concurrently -n Typescript,Chokidar \"yarn watch:ts\" \"yarn watch:dist\" -p \"[{name} - {time}]\" -c \"yellow\",\"blue\" ",
    "watch:ts": "tsc -w --preserveWatchOutput",
    "watch:dist": "node watcher.js",
    "copy": "node copier.js",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint": "eslint 'src/**/*.{js,ts,tsx,graphql}'",
    "gqlgen": "graphql-codegen --config codegen.yml"
  },
  "devDependencies": {
    "@babel/core": "7.13.15",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.13.13",
    "@babel/preset-typescript": "7.10.4",
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/core": "1.17.9",
    "@graphql-codegen/fragment-matcher": "2.0.1",
    "@graphql-codegen/near-operation-file-preset": "1.17.13",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-graphql-files-modules": "1.18.1",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@graphql-codegen/typescript-react-apollo": "2.2.3",
    "@graphql-codegen/visitor-plugin-common": "1.19.1",
    "@graphql-eslint/eslint-plugin": "0.9.1",
    "@testing-library/dom": "7.30.3",
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.6",
    "@testing-library/user-event": "13.1.2",
    "@types/glob": "7.1.3",
    "@types/react": "17.0.1",
    "@types/react-dom": "17.0.0",
    "@types/yup": "0.26.34",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "apollo-mocked-provider": "4.0.1",
    "babel-jest": "26.3.0",
    "chokidar": "3.5.1",
    "concurrently": "6.0.2",
    "eslint": "7.24.0",
    "eslint-config-prettier": "8.2.0",
    "eslint-plugin-react": "7.23.2",
    "jest": "26.6.0",
    "prettier": "2.2.1",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "@apollo/client": "3.3.0",
    "@material-ui/core": "4.11.3",
    "@material-ui/icons": "4.11.2",
    "emotion": "10.0.27",
    "formik": "2.1.5",
    "graphql": "15.5.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "yup": "0.28.3"
  }
}
