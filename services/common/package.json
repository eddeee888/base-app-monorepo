{
  "name": "@bam/common",
  "version": "1.0.0",
  "author": "Eddy",
  "license": "MIT",
  "scripts": {
    "build": "tsc --noEmit false",
    "watch": "concurrently -n Typescript,Chokidar \"yarn watch:ts\" \"yarn watch:dist\" -p \"[{name} - {time}]\" -c \"yellow\",\"blue\" ",
    "watch:ts": "tsc -w --preserveWatchOutput",
    "watch:dist": "node watcher.js",
    "copy": "node copier.js",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "gqlgen": "graphql-codegen --config codegen.yml",
    "add-gcg": "./scripts/install_gcg.sh"
  },
  "devDependencies": {
    "@graphql-codegen/add": "1.17.4",
    "@graphql-codegen/cli": "1.17.4",
    "@graphql-codegen/core": "1.17.4",
    "@graphql-codegen/near-operation-file-preset": "1.17.4",
    "@graphql-codegen/typescript": "1.17.4",
    "@graphql-codegen/typescript-graphql-files-modules": "1.17.4",
    "@graphql-codegen/typescript-operations": "1.17.4",
    "@graphql-codegen/typescript-react-apollo": "1.17.4",
    "@graphql-codegen/visitor-plugin-common": "1.17.4",
    "@types/react": "16.9.32",
    "@types/react-dom": "16.9.6",
    "@types/yup": "0.26.34",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "chokidar": "^3.3.1",
    "concurrently": "^5.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "graphql": "14.6.0",
    "prettier": "2.0.2",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@apollo/client": "3.1.1",
    "@material-ui/core": "4.9.14",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.53",
    "emotion": "10.0.27",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "yup": "0.28.3"
  }
}
