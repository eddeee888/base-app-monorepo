{
  "name": "@bam/common",
  "version": "1.0.0",
  "author": "Eddy",
  "license": "MIT",
  "scripts": {
    "build": "tsc --noEmit false",
    "test": "jest",
    "watch": "concurrently -n Typescript,Chokidar \"yarn watch:ts\" \"yarn watch:dist\" -p \"[{name} - {time}]\" -c \"yellow\",\"blue\" ",
    "watch:ts": "tsc -w --preserveWatchOutput",
    "watch:dist": "node watcher.js",
    "copy": "node copier.js",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint": "eslint 'src/**/*.{js,ts,tsx,graphql}'",
    "gqlgen": "graphql-codegen --config codegen.yml"
  },
  "devDependencies": {
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@graphql-codegen/cli": "1.20.1",
    "@graphql-codegen/core": "1.17.9",
    "@graphql-codegen/fragment-matcher": "2.0.1",
    "@graphql-codegen/near-operation-file-preset": "1.17.13",
    "@graphql-codegen/typescript": "1.20.2",
    "@graphql-codegen/typescript-graphql-files-modules": "1.18.1",
    "@graphql-codegen/typescript-operations": "1.17.14",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@graphql-codegen/visitor-plugin-common": "1.18.2",
    "@graphql-eslint/eslint-plugin": "0.8.0",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.4",
    "@testing-library/user-event": "12.1.7",
    "@types/autosuggest-highlight": "3.1.0",
    "@types/glob": "7.1.3",
    "@types/react": "17.0.1",
    "@types/react-dom": "17.0.0",
    "@types/yup": "0.26.34",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "apollo-mocked-provider": "4.0.1",
    "babel-jest": "26.3.0",
    "chokidar": "^3.3.1",
    "concurrently": "^5.1.0",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-react": "7.21.5",
    "jest": "26.4.2",
    "prettier": "2.0.2",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "@apollo/client": "3.3.0",
    "@material-ui/core": "4.9.14",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.53",
    "emotion": "10.0.27",
    "formik": "2.1.5",
    "graphql": "15.5.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "yup": "0.28.3"
  },
  "resolutions": {
    "@testing-library/dom": "7.24.4"
  }
}
