{
  "name": "@bam/client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "REACT_APP_ENVIRONMENT=development react-app-rewired start",
    "build": "REACT_APP_ENVIRONMENT=production react-app-rewired build",
    "analyse": "yarn build && yarn source-map-explorer 'build/static/js/*.js'",
    "test": "react-app-rewired test --transformIgnorePatterns \"./node_modules/(?!(@material-ui))/\"",
    "eject": "react-app-rewired eject",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "gqlgen": "graphql-codegen --config codegen.yml",
    "add-gcg": "./scripts/install_gcg.sh"
  },
  "dependencies": {
    "@apollo/client": "3.1.1",
    "@material-ui/core": "4.9.14",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.53",
    "emotion": "10.0.27",
    "eslint-plugin-react": "7.19.0",
    "formik": "2.1.5",
    "graphql": "14.6.0",
    "react": "16.13.1",
    "react-app-rewire-graphql-tag": "1.1.0",
    "react-dom": "16.13.1",
    "react-dropzone": "10.1.9",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.1",
    "route-codegen": "0.6.0",
    "yup": "0.28.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.4",
    "@graphql-codegen/core": "1.17.4",
    "@graphql-codegen/near-operation-file-preset": "1.17.4",
    "@graphql-codegen/typescript": "1.17.4",
    "@graphql-codegen/typescript-graphql-files-modules": "1.17.4",
    "@graphql-codegen/typescript-operations": "1.17.4",
    "@graphql-codegen/typescript-react-apollo": "1.17.4",
    "@graphql-codegen/visitor-plugin-common": "1.17.4",
    "@testing-library/react": "10.0.2",
    "@testing-library/react-hooks": "3.2.1",
    "@types/jest": "25.2.1",
    "@types/node": "12.7.8",
    "@types/react": "16.9.32",
    "@types/react-dom": "16.9.6",
    "@types/react-router": "5.1.5",
    "@types/react-router-dom": "5.1.3",
    "@types/yup": "0.26.34",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "babel-plugin-import": "1.12.2",
    "customize-cra": "0.8.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "prettier": "2.0.2",
    "react-app-rewired": "2.1.5",
    "react-test-renderer": "16.13.1",
    "source-map-explorer": "2.4.2",
    "typescript": "3.8.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
