{
  "name": "@bam/client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "REACT_APP_ENVIRONMENT=development react-app-rewired start",
    "build": "REACT_APP_ENVIRONMENT=production react-app-rewired build",
    "analyse": "yarn build && yarn source-map-explorer 'build/static/js/*.js'",
    "test": "react-app-rewired test --env=jest-environment-jsdom-sixteen",
    "eject": "react-app-rewired eject",
    "lint": "eslint 'src/**/*.{js,ts,tsx,graphql}'",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "gqlgen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "3.3.0",
    "@material-ui/core": "4.9.14",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.53",
    "@route-codegen/react": "0.1.1",
    "@route-codegen/utils": "0.1.1",
    "emotion": "10.0.27",
    "formik": "2.1.5",
    "graphql": "14.6.0",
    "react": "17.0.1",
    "react-app-rewire-graphql-tag": "1.1.0",
    "react-dom": "17.0.1",
    "react-dropzone": "10.1.9",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-scripts": "4.0.0",
    "subscriptions-transport-ws": "0.9.18",
    "yup": "0.28.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.19.4",
    "@graphql-codegen/core": "1.17.9",
    "@graphql-codegen/near-operation-file-preset": "1.17.13",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-graphql-files-modules": "1.18.1",
    "@graphql-codegen/typescript-operations": "1.17.12",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@graphql-codegen/visitor-plugin-common": "1.17.21",
    "@graphql-eslint/eslint-plugin": "0.7.1",
    "@graphql-tools/mock": "6.2.4",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.4",
    "@testing-library/user-event": "12.1.7",
    "@types/jest": "25.2.1",
    "@types/node": "12.7.8",
    "@types/react": "16.9.55",
    "@types/react-dom": "16.9.6",
    "@types/react-router": "5.1.5",
    "@types/react-router-dom": "5.1.3",
    "@types/yup": "0.26.34",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "apollo-mocked-provider": "4.0.1",
    "babel-plugin-import": "1.12.2",
    "customize-cra": "1.0.0",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.21.5",
    "jest-environment-jsdom-sixteen": "1.0.3",
    "jest-transform-graphql": "2.1.0",
    "prettier": "2.0.2",
    "react-app-rewired": "2.1.5",
    "react-test-renderer": "16.13.1",
    "source-map-explorer": "2.4.2",
    "typescript": "4.0.3"
  },
  "resolutions": {
    "graphql": "14.6.0",
    "@apollo/client": "3.3.0",
    "@testing-library/dom": "7.24.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "testMatch": [
      "<rootDir>/src/**/*/?(*.)test.(js|ts)?(x)"
    ]
  }
}
