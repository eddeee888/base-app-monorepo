{
  "root": "apps/graph",
  "sourceRoot": "apps/graph/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/graph",
        "main": "apps/graph/src/main.ts",
        "tsConfig": "apps/graph/tsconfig.app.json",
        "assets": ["apps/graph/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/graph/src/environments/environment.ts",
              "with": "apps/graph/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "graph:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/graph/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/graph"],
      "options": {
        "jestConfig": "apps/graph/jest.config.js",
        "passWithNoTests": true
      }
    },
    "graphql-codegen": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": ["apps/graph/src/graphql/generated"],
      "options": {
        "command": "graphql-codegen --config ./apps/graph/graphql-codegen.yml"
      }
    },
    "typecheck": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "tsc -b apps/graph/tsconfig.app.json apps/graph/tsconfig.spec.json"
      }
    }
  },
  "tags": ["app:graph"]
}
