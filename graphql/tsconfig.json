{
  "compilerOptions": {
    "lib": ["es2016", "esnext.asynciterable"],
    "baseUrl": ".",
    "paths": {
      "src": ["./src"]
    },
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noImplicitAny": true
  },
  "exclude": ["./node_modules", "./src/**/generated/*.ts"]
}
