{
  "name": "bra-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon",
    "serve": "ts-node -r tsconfig-paths/register src/web/server.ts",
    "prisma:gen": "prisma generate --env-file .env.dev",
    "prisma:deploy": "prisma deploy --env-file .env.dev",
    "prisma:seed": "prisma seed --reset --env-file .env.dev",
    "gg": "graphqlgen",
    "check:ts": "tsc -p tsconfig.json",
    "test": "yarn test:prisma-delete && yarn test:prisma-setup && yarn test:jest",
    "test:jest": "jest -i",
    "test:prisma-setup": "prisma deploy --force --env-file .env.test",
    "test:prisma-delete": "prisma delete --force --env-file .env.test",
    "format:prettier": "prettier --config .prettierrc.json --write \"src/**/*.ts*\"",
    "format:check": "eslint 'src/**/*.ts'",
    "format:eslint": "eslint --fix 'src/**/*.ts'",
    "format": "yarn format:prettier && yarn format:eslint"
  },
  "author": "Eddy Nguyen",
  "license": "ISC",
  "dependencies": {
    "@bit/eddeee888.base-react-app-utils.graphql": "^0.0.1",
    "@bit/eddeee888.base-react-app-utils.validations": "^0.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.1",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/yup": "^0.26.4",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.2.0",
    "esm": "^3.2.14",
    "graphql": "^14.3.1",
    "graphql-import": "^0.7.1",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "^8.4.0",
    "prisma-client-lib": "1.33.0",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "graphqlgen": "0.5.1",
    "jest": "^24.7.1",
    "jest-express": "^1.10.0",
    "nodemon": "^1.18.9",
    "prisma": "1.33.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.7.0",
    "typescript": "^3.2.2"
  }
}
